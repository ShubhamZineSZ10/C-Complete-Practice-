/*  1 b 2 d
 *    1 b 2
 *      1 b
 *        1*/ 

#include<stdio.h>
void main(){
	int rows;
	printf("Enter no of rows : ");
	scanf("%d",&rows);

	for( int i=1;i<=rows;i++){
		int c=1,n=1;
		for(int j=1;j<i;j++){
			printf("\t"<F2>);
		}
		for(int k=rows;k>i;k--){
			if((i+j)%2==0){
				printf("%c\t",64+c);
			}else{
				printf("%d\t",n);
				n++;

			}
			c++;
		}
		printf("\n");
	}
	
}
